{#
    params : 
        - user : User object
#}
<form class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label">{{ 'app.name' | trans | capitalize }}</label>
        <div class="col-sm-10">
            <p class="form-control-static">{{ user.name }}</p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">{{ 'app.email' | trans | capitalize }}</label>
        <div class="col-sm-10">
            <p class="form-control-static">{{ user.email }}</p>
        </div>
    </div>
</form>